<div class="standard-page">
  <h1>Welcome, {{admin.fullName}}</h1>
  <p>What would you like to do today?</p>
  <div class="profile-actions">
    <div class="profile-boxes">
      <img src="/images/single.png" class="profile-box-images" />
      <button class="profile-buttons"><a href="/admin/create-admin">Create Admin</a></button>
    </div>
    <div class="profile-boxes">
      <img src="/images/single.png" class="profile-box-images" />
      <button class="profile-buttons"><a href="/admin/create-employee">Create
          New Employee</a></button>
    </div>
    <div class="profile-boxes">
      <img src="/images/group.jpeg" class="profile-box-images" />
      <button class="profile-buttons"><a href="/admin/view-all">Company Roster</a></button>
    </div>
    <div class="profile-boxes">
      <img src="/images/schedule.png" class="profile-box-images" />
      <button class="profile-buttons"><a
          href="/admin/schedule/create-schedule"
        >Create a Schedule</a></button>
    </div>
    <div class="profile-boxes">
      <img src="/images/schedule-view.png" class="profile-box-images" />
      <button class="profile-buttons"><a
          href="/admin/schedule/view-schedule"
        >Schedule View</a></button>
    </div>
  </div>
  <form method="get" action="/admin/logout">
    <button type="submit">Log Out</button>
  </form>
</div>